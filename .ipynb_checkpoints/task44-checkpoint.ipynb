   "source": [
    "import random\n",
    "import pandas as pd\n",
    "lst = ['robot'] * 10\n",
    "lst += ['human'] * 10\n",
    "random.shuffle(lst)\n",
    "data = pd.DataFrame({'whoAmI':lst})\n",
    "data.loc[data['whoAmI'] == 'robot', 'robot' ] = '1'\n",
    "data.loc[data['whoAmI'] == 'human', 'human' ] = '1'\n",
    "data = data.fillna(0)\n",
    "data.head(20)"
   ]
